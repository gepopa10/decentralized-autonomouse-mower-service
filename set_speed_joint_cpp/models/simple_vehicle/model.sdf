<?xml version="1.0" ?>
<sdf version='1.4'>
  <model name="simple_vehicle">
        <static>0</static>
        <!-- Give the base link a unique name -->
        <link name="vehicle_body">

            <!-- Offset the base by half the lenght of the cylinder -->
            <pose>0 0 0 0 0 0</pose>
            <inertial>
              <mass>1.0</mass>
              <inertia>
                <ixx>0.0006</ixx>
                <iyy>0.0006</iyy>
                <izz>0.0006</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
              </inertia>
            </inertial>


            <collision name="vehicle_body_collision">
              <geometry>
                <box>
                  <size>0.060 0.060 0.060</size>
                </box>
              </geometry>
                <surface>
		<friction>
		<ode>
		    <mu>0.0</mu>
		    <mu2>0.0</mu2>
		</ode>
		</friction>
		<contact>
		<ode>
		   <kp>1000000.0</kp>
		   <kd>1000000.0</kd>
		   <!--<max_vel>0.1</max_vel>-->
		   <min_depth>0</min_depth>
		</ode>
		</contact>
	      </surface>
            </collision>

            <!-- The visual is mostly a copy of the collision -->
            <visual name="vehicle_body_visual">
              <geometry>
                <box>
                  <size>0.060 0.060 0.060</size>
                </box>
              </geometry>
              <material>
                <script>
                  <uri>file://media/materials/scripts/gazebo.material</uri>
                  <name>Gazebo/Green</name>
                </script>
              </material>
            </visual>
        </link>

        <!-- Give the base link a unique name -->
        <link name="left_wheel">

            <!-- Vertically offset the top cylinder by the length of the bottom
                cylinder and half the length of this cylinder. -->
            <pose>0 0.035 0 1.57 0 0</pose>

            <inertial>
              <mass>0.1</mass>
              <inertia>
                <ixx>2.33333333333e-05</ixx>
                <iyy>2.33333333333e-05</iyy>
                <izz>4.5e-05</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
              </inertia>
            </inertial>

            <collision name="left_wheel_collision">
              <geometry>
                <cylinder>
                  <!-- Radius and length provided by Velodyne -->
                  <radius>0.031</radius>
                  <length>0.005</length>
                </cylinder>
              </geometry>
              <surface>
		<friction>
		<ode>
		    <mu>30.0</mu>
		    <mu2>30.0</mu2>
		</ode>
		</friction>
		<contact>
		<ode>
		   <kp>1000000.0</kp>
		   <kd>1000000.0</kd>
		   <!--<max_vel>0.1</max_vel>-->
		   <min_depth>0.0</min_depth>
		</ode>
		</contact>
	      </surface>
            </collision>

            <!-- The visual is mostly a copy of the collision -->
            <visual name="left_wheel_visual">
              <geometry>
                <cylinder>
                  <radius>0.031</radius>
                  <length>0.005</length>
                </cylinder>
              </geometry>
              <material>
                <script>
                  <uri>file://media/materials/scripts/gazebo.material</uri>
                  <name>Gazebo/Blue</name>
                </script>
              </material>
            </visual>
        </link>

	<!-- Give the base link a unique name -->
        <link name="right_wheel">

            <!-- Vertically offset the top cylinder by the length of the bottom
                cylinder and half the length of this cylinder. -->
            <pose>0 -0.035 0 1.57 0 0</pose>

            <inertial>
              <mass>0.1</mass>
              <inertia>
                <ixx>2.33333333333e-05</ixx>
                <iyy>2.33333333333e-05</iyy>
                <izz>4.5e-05</izz>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyz>0</iyz>
              </inertia>
            </inertial>

            <collision name="right_wheel_collision">
              <geometry>
                <cylinder>
                  <!-- Radius and length provided by Velodyne -->
                  <radius>0.031</radius>
                  <length>0.005</length>
                </cylinder>
              </geometry>
	      <surface>
		<friction>
		<ode>
		    <mu>30.0</mu>
		    <mu2>30.0</mu2>
		</ode>
		</friction>
		<contact>
		<ode>
		   <kp>1000000.0</kp>
		   <kd>1000000.0</kd>
		   <!--<max_vel>0.1</max_vel>-->
		   <min_depth>0.0</min_depth>
		</ode>
		</contact>
	      </surface>
            </collision>

            <!-- The visual is mostly a copy of the collision -->
            <visual name="right_wheel_visual">
              <geometry>
                <cylinder>
                  <radius>0.031</radius>
                  <length>0.005</length>
                </cylinder>
              </geometry>
              <material>
                <script>
                  <uri>file://media/materials/scripts/gazebo.material</uri>
                  <name>Gazebo/Red</name>
                </script>
              </material>
            </visual>
        </link>


        <!-- Each joint must have a unique name -->
        <joint type="revolute" name="left_wheel_joint">
          <pose>0 0 0 0 0 0</pose>
          <parent>vehicle_body</parent>
          <child>left_wheel</child>
          <!-- The axis defines the joint's degree of freedom -->
          <axis>
            <xyz>0 1 0</xyz>
            <!-- Limit refers to the range of motion of the joint -->
            <limit>
              <!-- Use a very large number to indicate a continuous revolution -->
              <lower>-10000000000000000</lower>
              <upper>10000000000000000</upper>
            </limit>
          </axis>
        </joint>

        <!-- Each joint must have a unique name -->
        <joint type="revolute" name="right_wheel_joint">
          <pose>0 0 0 0 0 0</pose>
          <parent>vehicle_body</parent>
          <child>right_wheel</child>
          <!-- The axis defines the joint's degree of freedom -->
          <axis>
            <xyz>0 1 0</xyz>
            <!-- Limit refers to the range of motion of the joint -->
            <limit>
              <!-- Use a very large number to indicate a continuous revolution -->
              <lower>-10000000000000000</lower>
              <upper>10000000000000000</upper>
            </limit>
          </axis>
        </joint>
          
	<plugin name="set_speed_joint_plugin" filename="libset_speed_joint_plugin.so">
		<namespace_model>simple_robot</namespace_model>
		<activate_pid_control>yes</activate_pid_control>
		<wheel_kp>0.1</wheel_kp>
		<wheel_ki>0.0</wheel_ki>
		<wheel_kd>0.0</wheel_kd>
	</plugin>

  </model>
</sdf>
